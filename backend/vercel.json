{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python@3.1.30"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "PYTHONUNBUFFERED": "1"
    }
}
